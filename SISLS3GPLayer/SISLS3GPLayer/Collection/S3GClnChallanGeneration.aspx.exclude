                            <tr>
                                <td align="center">
                                    &nbsp;
                                    <asp:RequiredFieldValidator ID="rfvddlLOB" runat="server" ControlToValidate="ddlLOB"
                                        CssClass="styleMandatoryLabel" Display="None" InitialValue="0" SetFocusOnError="True"
